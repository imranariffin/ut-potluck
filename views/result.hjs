<!DOCTYPE html>
<html>
  <head>
  <title>{{title}}</title>
  <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link rel='stylesheet' href='/stylesheets/c3.min.css' />
  <script src="/javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.min.js"></script>
  <script src="/javascripts/d3.min.js"></script>
  <script src="/javascripts/c3.min.js"></script>
  </head>
  <body>

    {{>navbar}}
    <div class="container">
    <h2>Result</h2>
    <br>
      <div class="row text-center">
        <div class="col-md-4">
          <h3>Paling Kacak</h3>
          <div id="chart1"></div>
          <br>
        </div>
        <div class="col-md-4">
          <h3>Paling Hensem</h3>
          <div id="chart2"></div>
          <br>
        </div>
        <div class="col-md-4">
          <h3>Paling Macho</h3>
          <div id="chart3"></div>
          <br>
        </div>
      </div>
      
    </div>

    <script>
      var chart1 = c3.generate({
          bindto: '#chart1',
          data: {
              bindto: '#chart1',
              columns: [
                  ['person1', 14],
                  ['person2', 130],
                  ['person3', 12]
              ],
              type: 'bar'
          },
          bar: {
              width: {
                  ratio: 0.6 // this makes bar width 50% of length between ticks
              }
              // or
              //width: 100 // this makes bar width 100px
          }
      });

      var chart2 = c3.generate({
          bindto: '#chart2',
          data: {
              columns: [
                  ['person1', 14],
                  ['person2', 130],
                  ['person3', 12]
              ],
              type: 'bar'
          },
          bar: {
              width: {
                  ratio: 0.6 // this makes bar width 50% of length between ticks
              }
              // or
              //width: 100 // this makes bar width 100px
          }
      });

      var chart3 = c3.generate({
          bindto: '#chart3',
          data: {
              columns: [
                  ['person1', 14],
                  ['person2', 130],
                  ['person3', 12]
              ],
              type: 'bar'
          },
          bar: {
              width: {
                  ratio: 0.6 // this makes bar width 50% of length between ticks
              }
              // or
              //width: 100 // this makes bar width 100px
          }
      });
    </script>
  </body>
</html>